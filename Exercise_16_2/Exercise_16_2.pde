//2. Use multiple translations to reposition a series of shapes.


size(500, 500);
fill(0);
stroke(255);


rect(45, 45, 90, 90);
ellipse(50, 50, 50, 50);
translate(100, 100);

rect(45, 45, 90, 90);
ellipse(50, 50, 50, 50);
translate(-50, -50);

rect(45, 45, 90, 90);
ellipse(50, 50, 50, 50);
translate(-200, -100);


